<UserControl x:Class="Phrike.GroundControl.Views.Analysis"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:oxy="http://oxyplot.org/wpf"
             xmlns:viewModels="clr-namespace:Phrike.GroundControl.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">

    <UserControl.Resources>
        <viewModels:AnalysisViewModel x:Key="AnalysisViewModel" />
    </UserControl.Resources>

    <Grid DataContext="{StaticResource AnalysisViewModel}" Margin="-350,-106,-325,-123">
        <oxy:PlotView Model="{Binding SensorPulse}" Height="200" Width="500" Margin="54,121,421,208"/>
    </Grid>
</UserControl>
